citEntry(entry = "manual",
    title = "{exampledata}: A ...",
    author = "Tyler W. Rinker",
    address = "Buffalo, New York",
    note = "version 0.0.1",
    year = "2018",
    url = "http://github.com/trinker/exampledata", 
    textVersion  = paste("Rinker, T. W. (2018).",
        "exampledata: ...",
        "version 0.0.1. Buffalo, New York.",
        "http://github.com/trinker/exampledata")
)